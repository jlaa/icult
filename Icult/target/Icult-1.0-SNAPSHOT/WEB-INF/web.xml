<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterUsuarioServlet</servlet-name>
        <servlet-class>com.icult.controller.RegisterUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginUsuarioServlet</servlet-name>
        <servlet-class>com.icult.controller.LoginUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.icult.controller.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlterarUsuarioServlet</servlet-name>
        <servlet-class>com.icult.controller.AlterarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.icult.controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProdutoController</servlet-name>
        <servlet-class>com.icult.controller.ProdutoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>produto</servlet-name>
        <servlet-class>com.icult.controller.ProdutoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>produto</servlet-name>
        <url-pattern>/produto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterUsuarioServlet</servlet-name>
        <url-pattern>/RegisterUsuarioServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>HomeServlet</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CadastrarCartaoServlet</servlet-name>
        <servlet-class>com.icult.controller.CadastrarCartaoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlterarCartaoServlet</servlet-name>
        <servlet-class>com.icult.controller.AlterarCartaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginUsuarioServlet</servlet-name>
        <url-pattern>/LoginUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/HomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlterarUsuarioServlet</servlet-name>
        <url-pattern>/AlterarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProdutoController</servlet-name>
        <url-pattern>/ProdutoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarCartaoServlet</servlet-name>
        <url-pattern>/CadastrarCartaoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlterarCartaoServlet</servlet-name>
        <url-pattern>/AlterarCartaoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
